{
	"name": "WeatherSink",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"filename": {
				"type": "string",
				"defaultValue": "@{formatDateTime(addDays(utcNow(), add(mul(-7, add(item(), 1)), -7)), 'yyyyMMdd')}"
			}
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat('Weather_History_', dataset().filename, '.json')",
					"type": "Expression"
				},
				"folderPath": "Bronze/History",
				"fileSystem": "phsblob"
			}
		},
		"schema": {}
	}
}